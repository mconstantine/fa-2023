{
  "name": "fa-2023",
  "version": "1.0.0",
  "description": "Financial Analysis 2023",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch \"./src/**\" --ext \"ts, json\" --ignore \"./src/**/*.test.ts\" --exec \"ts-node --swc ./src/index.ts\"",
    "build": "tsc",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "f() { typeorm-ts-node-commonjs migration:generate -d ./src/AppDataSource.ts \"./src/migrations/$@\"; }; f",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/AppDataSource.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/AppDataSource.ts"
  },
  "author": "Mauro Constantinescu",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "routing-controllers": "^0.10.4",
    "typeorm": "^0.3.17"
  }
}
