{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-redeclare": 0
  },
  "overrides": [
    {
      "files": "**/*.test.ts",
      "rules": {
        "@typescript-eslint/no-floating-promises": 0
      }
    }
  ]
}
